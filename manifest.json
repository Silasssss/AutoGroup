{
    "manifest_version": 2,
    "name": "AutoGroup",
    "description": "Charge votre emploie en fonction de votre groupe", 
    "version": "1.0",
    "permissions": [ 
     "http://*/*",
     "https://*/*",
     "storage"
    ],
    "browser_action": {   
     "default_icon" : "img/icon.png",
     "default_popup": "app/popup.html"
    },
    "icons": {
     "128" : "img/icon.png"
    },
       "content_scripts": [
        {
          "matches": ["https://planning.univ-rennes1.fr/*"],
          "js": ["scripts/load.js"]
        }
      ]
   }